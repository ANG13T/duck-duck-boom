{
  "name": "duck-duck-boom",
  "displayName": "Duck Duck Boom",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.68.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:duck-duck-boom.helloWorld",
    "onCommand:duck-duck-boom.quickInput"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "duck-duck-boom.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "duck-duck-boom.quickInput",
        "title": "Quick Input"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/node-fetch": "^2.6.2",
    "@types/vscode": "^1.68.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.18.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "fs": "^0.0.1-security",
    "node-fetch": "^2.6.7",
    "path": "^0.12.7",
    "stream": "^0.0.2"
  }
}
